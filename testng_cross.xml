<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Cosmoslots" parallel="tests" thread-count="5">

    <parameter name="browser" value="chrome" />
    
    <test name="Cosmoslot_Chrome">
        <parameter name="browser" value="chrome" />
        <classes>
            <class name="com.Cosmoslots.testCases.PlayerDeletionHistory"/>
        </classes>
    </test>
    
    <!-- Add another test for parallel execution with a different browser -->
    <test name="Cosmoslot_Firefox">
        <parameter name="browser" value="firefox" />
        <classes>
            <class name="com.Cosmoslots.testCases.PlayerDeletionHistory"/>
        </classes>
    </test>

    <!-- You can add more tests for other browsers if needed -->

</suite>

